{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "k-mit/versionable",
                "version": "dev-master",
                "source": {
                    "url": "git://k-mit/versionable.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0" : {
                        "k-mit\\Versionable" : "src"
                    }
                }
            }
        }
    ],
    "name": "k-mit/versionable",
    "license": "MIT",
    "description": "Allows to create Laravel 4 / 5 Model versioning and restoring",
    "keywords": ["model", "laravel", "ardent", "version", "history", "restore"],
    "homepage": "http://github.com/mpociot/versionable",
    "authors": [
        {
            "name": "Pontus Kindblad",
            "email": "pontus@k-mit.se"
        }
    ],
    "support": {
        "source": "https://github.com/k-mit/versionable"
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "mockery/mockery": "dev-master",
        "illuminate/database": "~5.0",
        "illuminate/events": "~5.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/VersionableTestCase.php"
        ],
        "psr-0": {
            "k-mit\\Versionable": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}